
public class BestTimeToBuyAndSellStockII {
	
	public static void bestTimeToBuyAndSell(int arr[]) {
		int profit = 0;
		
		for (int i = 0; i < arr.length; i++) {
			if(arr[i] < arr[i - 1]) {
				profit += arr[i] - arr[i -1];
			}
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
