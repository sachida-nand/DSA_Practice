package anotherExample;

public class reverseDigits {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		ReverceDigit()

	}
	
   public static int ReverceDigit(int digit) {
	   int num = digit;
	   
	   if(num !=0) {
		   int reverse = num % 10;
		   num = num /10;
		   
		   System.out.print(reverse);
	   }
   }

}
