
public class RotateArrayByKElement {
	
	public static void rotateElementByKth(int arr[]) {
		int n = arr.length; //n=5 
		int k = 2;
		int r = arr.length - 1;
		int startIndex = n - k; // 5 - 2 = 3
		
		while (startIndex < ) {
			
		}
		
		
		
		
		
		for (int i = startIndex ; i < arr.length; i++) {
			int temp = arr[i];
			System.out.println("start index "+arr[n - 1 - i]);
			arr[i] = arr[n - 1 - i];
			arr[n - 1 - i] = temp;
			
//			System.out.println("start index "+arr[i]);
		}
		
		for (int i = 0; i < arr.length; i++) {
			System.out.print(" "+ arr[i]);
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int arr[] = {1, 3, 5, 7, 9};
		
//		Output: 7 9 1 3 5
		
		rotateElementByKth(arr);
	}

}
