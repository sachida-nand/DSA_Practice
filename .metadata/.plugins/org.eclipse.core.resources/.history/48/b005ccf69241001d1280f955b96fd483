package anotherExample;

public class reverseDigits {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		ReverceDigit(3658);

	}
	
   public static int ReverceDigit(int digit) {
	   int num = digit;
	   int reverse = 0;
	   
	   
	   
	   if(num !=0) {
		   reverse = num % 10;
		   num = num / 10;
	   }
	   System.out.print(reverse);
	   return 1;
   }

}
