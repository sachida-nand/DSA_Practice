
public class TransposeOfMatrix {
	public static void transposeMatrix(int arr[][]) {
		for (int i = 0; i < arr.length; i++) {
			for (int j = i; j < arr.length; j++) {
				int temp = arr[i][j];
				arr[i][j] = arr[j][i];
				arr[j][i] = temp;
			}
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
